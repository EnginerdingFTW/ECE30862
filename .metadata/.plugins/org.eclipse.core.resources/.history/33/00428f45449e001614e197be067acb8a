import java.util.Timer;

public class MultiTimer extends java.util.TimerTask
{
	float delay = 1000f;
    public void run()
    {
    	while (true)
    	{
	    	try
	    	{
	    		Thread.sleep((long) (1000));
	    	}
	    	catch (Exception e)
	    	{
	    		System.out.println("ERROR");
	    	}
    	}
    }
}
